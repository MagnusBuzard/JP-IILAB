#include <stdlib.h>
#include <iostream>
#include <conio.h>


using namespace std;

double x, y, z;


void main()
{
	cout << "Podaj wartosc x = "; cin >> x;
	if (x >= 5 && x <= 100)
	{
		z = (1.0 / 3.0);
		y = pow((x * x + 2) / (0.7 * x), z);
		cout << "\nWartosc funkcji w punkcie x = " << x << " wynosi y = " << y << endl;
	}
	else
	{
		if (x <= 0)
		{
			z = pow(3, (1.0 / 2.0));
			y = (3 * x * x - 0.1) / z;
			cout << "\nWartosc funkcji w punkcie x = " << x << " wynosi y = " << y << endl;
		}
		else
		{
			cout << "\nWartosc funkcji w punkcie x = " << x << " wynosi y = " << x << endl;
		}
	}
	_getch();
}
